<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleTextView">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="maxLength" format="integer" />
        <attr name="shadow" format="boolean" />
        <attr name="textStyle" format="enum">
            <enum name="FILL" value="1" />
            <enum name="EMPTY" value="2" />
        </attr>
        <attr name="letterSpace" format="float" />
        <attr name="offset" format="dimension" />
        <attr name="circle_size" format="enum">
            <enum name="match_parent" value="0" />
            <enum name="wrap_text" value="1" />
        </attr>
    </declare-styleable>
</resources>